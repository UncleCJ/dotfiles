<?xml version="1.0" encoding="UTF-8"?>
 <settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
 <pluginGroups>
 </pluginGroups>
 <proxies>
 </proxies>
 <servers>
 </servers>
<mirrors>
   <mirror>
     <id>artifactory</id>
     <mirrorOf>*</mirrorOf>
     <url> http://prdmvn01:8081/artifactory/repo </url>
     <name>Artifactory</name>
   </mirror>
 </mirrors>
 <profiles>
   <profile>
     <id>default-fgirot-profile</id>
     <activation>
       <activeByDefault>true</activeByDefault>
     </activation>
     <repositories>
       <repository>
         <id>artifactory-fgirot</id>
         <url>http://prdmvn01:8081/artifactory/repo</url>
         <snapshots>
           <enabled>true</enabled>
         </snapshots>
         <releases>
           <enabled>true</enabled>
         </releases>
       </repository>
     </repositories>
   </profile>
 </profiles>
</settings>
